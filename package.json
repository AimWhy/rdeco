{
  "name": "rdeco",
  "version": "2.3.25",
  "description": "基于响应式的 React 应用开发框架",
  "keywords": [
    "react",
    "flux",
    "hook",
    "state",
    "useReducer",
    "rxjs"
  ],
  "licenses": "MIT",
  "author": "shihong <112365362@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kinop112365362/structured-react-hook"
  },
  "types": "./types",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env NODE_ENV=production babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "build:docs": "cd website && yarn build && cd ..",
    "git:merge": "git co master && git pull && git co main && git merge master && git push && git co master",
    "clean": "rimraf lib dist es coverage types",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"docs/**/*.md\"",
    "lint": "eslint src --fix",
    "pretest": "npm run lint",
    "test": "jest",
    "coverage": "codecov",
    "gcg": "git co main",
    "gcs": "git co master",
    "pub": "rm -rf .npmrc && node ./bin/publish.mjs",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.12.1",
    "react": "^16.8.3 || ^17"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-react-display-name": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.12.1",
    "@emotion/babel-plugin": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@shopify/jest-dom-mocks": "^3.0.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/react-native": "^7.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "codecov": "^3.8.0",
    "commander": "^8.2.0",
    "create-react-class": "^15.7.0",
    "cross-env": "^7.0.2",
    "es3ify": "^0.2.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "router5-plugin-logger": "^8.0.1",
    "rxjs": "^7.4.0",
    "validate-npm-package-name": "^3.0.0"
  }
}